@page "/abountme"


<div class="container-fluid divAbountME alert-dark">
	<div class="d-flex justify-content-center">
		<div id="carouselExampleFade" class="carousel carousel-dark carousel-fade" data-bs-ride="carousel">
			<div class="carousel-inner">
				<div class="carousel-item active align-content-center">
					<img src="AbountMe/Slide1.png" class="rounded mx-auto d-block bg-fon imageCarousel">
				</div>
				<div class="carousel-item align-content-center">
					<img src="AbountMe/Slide2.jpg" class="rounded mx-auto d-block bg-fon imageCarousel">
				</div>
				<div class="carousel-item align-content-center">
					<img src="AbountMe/Slide1.jpg" class="rounded mx-auto d-block bg-fon imageCarousel">
				</div>
			</div>
		</div>
	</div>
	<div class="d-flex justify-content-between">
		<button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="prev">
			<span class="carousel-control-prev-icon" aria-hidden="true"></span>
			<span class="visually-hidden">Предыдущий</span>
		</button>
		<button class="carousel-control-next" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="next">
			<span class="carousel-control-next-icon" aria-hidden="true"></span>
			<span class="visually-hidden">Следующий</span>
		</button>
	</div>
</div>

